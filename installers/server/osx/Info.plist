<plist version="1.0">
    <dict>
        <key>CFBundleDevelopmentRegion</key>
        <string>English</string>
        <key>CFAllowMixedLocations</key>
        <string>true</string>
        <key>CFBundleExecutable</key>
        <string>go-server</string>
        <key>CFBundleIconFile</key>
        <string>go-server.icns</string>
        <key>CFBundleInfoDictionaryVersion</key>
        <string>6.0</string>
        <key>CFBundleName</key>
        <string>Go Server</string>
        <key>CFBundlePackageType</key>
        <string>APPL</string>
        <key>CFBundleIdentifier</key>
        <string>com.thoughtworks.studios.cruise.server</string>
        <key>CFBundleSignature</key>
        <string>????</string>
        <key>CFBundleGetInfoString</key>
        <string>Go Server @VERSION@. Copyright ThoughtWorks Inc., (c) 2000-2013</string>
        <key>CFBundleShortVersionString</key>
        <string>@VERSION@</string>
        <key>CFBundleVersion</key>
        <string>@REGVER@</string>
        <key>LSMultipleInstancesProhibited</key>
        <string>true</string>
        <key>Java</key>
        <dict>
            <key>ClassPath</key>
            <string>$APP_PACKAGE/go.jar</string>
            <key>JVMVersion</key>
            <string>1.6+</string>
            <key>MainClass</key>
            <string>com.thoughtworks.go.server.util.GoMacLauncher</string>
            <key>WorkingDirectory</key>
            <string>$APP_PACKAGE</string>
            <key>Properties</key>
            <dict>
                <key>com.apple.mrj.application.apple.menu.about.name</key>
                <string>Go Server</string>
                <key>apple.laf.useScreenMenuBar</key>
                <string>true</string>
                <key>cruise.server.mem</key>
                <string>512</string>
                <key>cruise.server.maxmem</key>
                <string>1024</string>
                <key>cruise.server.permgen</key>
                <string>128</string>
                <key>cruise.server.maxpermgen</key>
                <string>256</string>
                <key>cruise.server.lang</key>
                <string>en</string>
                <key>cruise.server.country</key>
                <string>US</string>
            </dict>
        </dict>
    </dict>
</plist>

