<!DOCTYPE HTML>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <%- @view_title ||= "Agents" -%>
  <%= render :partial => "shared/head" %>
</head>
<body id="<%= page_name %>" class="<%= page_name %>" <%= render "shared/body_data_attrs" %>>
<div class="page-wrap">
  <div id="body_bg">
    <%= render :partial => "shared/header" %>
    <div id='body_content'>
      <div class="messaging_wrapper" id="messaging_wrapper">
        <%= render :partial => "shared/flash_message" %>
      </div>
      <div class="page_header">
        <div class="row">
          <ul class="entity_title">
            <li class="last"><h1><%= @agent.getHostname() -%></h1></li>
          </ul>
        </div>
      </div>
      <%= content_wrapper_tag %>
      <div class="row">
        <%= yield %>
      </div>
      <%= end_content_wrapper %>
    </div>
  </div>
</div>
<%= render :partial => 'navigation_elements/footer' %>
</body>
</html>
