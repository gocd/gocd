<plist version="1.0">
    <dict>
        <key>CFBundleDevelopmentRegion</key>
        <string>English</string>
        <key>CFAllowMixedLocations</key>
        <string>true</string>
        <key>CFBundleExecutable</key>
        <string>go-server</string>
        <key>CFBundleIconFile</key>
        <string>go-server.icns</string>
        <key>CFBundleInfoDictionaryVersion</key>
        <string>6.0</string>
        <key>CFBundleName</key>
        <string>Go Server</string>
        <key>CFBundlePackageType</key>
        <string>APPL</string>
        <key>CFBundleIdentifier</key>
        <string>com.thoughtworks.studios.cruise.server</string>
        <key>CFBundleSignature</key>
        <string>????</string>
        <key>CFBundleGetInfoString</key>
        <string>Go Server @version@.@distVersion@. Copyright ThoughtWorks Inc., (c) 2000-@currentYear@</string>
        <key>CFBundleShortVersionString</key>
        <string>@version@.@distVersion@</string>
        <key>CFBundleVersion</key>
        <string>@distVersion@</string>
        <key>LSMultipleInstancesProhibited</key>
        <string>true</string>
        <key>JavaX</key>
        <dict>
            <key>ClassPath</key>
            <string>$APP_PACKAGE/go.jar</string>
            <key>MainClass</key>
            <string>com.thoughtworks.go.server.util.GoMacLauncher</string>
            <key>Properties</key>
            <dict>
                <key>com.apple.mrj.application.apple.menu.about.name</key>
                <string>Go Server</string>
                <key>apple.laf.useScreenMenuBar</key>
                <string>true</string>
                <key>cruise.server.mem</key>
                <string>512</string>
                <key>cruise.server.maxmem</key>
                <string>1024</string>
                <key>cruise.server.permgen</key>
                <string>128</string>
                <key>cruise.server.maxpermgen</key>
                <string>256</string>
                <key>cruise.server.lang</key>
                <string>en</string>
                <key>cruise.server.country</key>
                <string>US</string>
                <key>go.server.mac.go.jar.dir</key>
                <string>$APP_PACKAGE</string>
            </dict>
        </dict>
    </dict>
</plist>
