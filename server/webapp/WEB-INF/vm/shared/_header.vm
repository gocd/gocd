          <link href="$req.getContextPath()/$pathResolver.getAssetPath("css/${css_name}.css")" rel="stylesheet"
                type="text/css"/>
        #end
    #end

  <link rel="stylesheet" type="text/css" href="$req.getContextPath()/$concatenatedNewThemeCssFilePath" media="all"/>
