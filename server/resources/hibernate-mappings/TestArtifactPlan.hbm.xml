<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.thoughtworks.go.config" default-access="field" default-lazy="false">
    <class name="com.thoughtworks.go.config.TestArtifactPlan" table="artifactPlans" polymorphism="explicit">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>

        <property name="buildId"/>
        <property name="src"/>
        <property name="dest"/>
        <property name="artifactType">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">com.thoughtworks.go.domain.ArtifactType</param>
                <param name="type">12</param>
            </type>
        </property>

    </class>
</hibernate-mapping>

