<!DOCTYPE HTML>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
  <%- @view_title = 'Preferences' -%>
  <%= render :partial => "shared/head" %>
</head>
<body id="<%= page_name %>" class="<%= page_name %>" <%= render "shared/body_data_attrs" %>>
<div class="page-wrap">
  <div id="body_bg">

    <%= render :partial => "shared/header" %>

    <div id='body_content'>
      <div class="messaging_wrapper" id="messaging_wrapper">
        <%= render :partial => "shared/flash_message" %>
      </div>

      <div class="page_header">
        <div class="row">
        <h1 class="entity_title">Preferences</h1>
      </div>
    </div>

    <div class="bd-container rounded-corner-for-pipeline admin-rounded-container row">
      <b class="c1"></b><b class="c2"></b><b class="c3"></b><b class="c4"></b>

      <div class="sub_tab_container rounded-corner-for-tab-container">
        <b class="c1"></b><b class="c2"></b><b class="c3"></b><b class="c4"></b>

        <div class="sub_tabs_container">
          <ul>
            <li>
              <%= link_to 'Notifications', preferences_notifications_path %>
            </li>
            <li class="current_tab">
              <%= link_to 'OAuth Access Tokens', url_for(:controller => 'user_tokens', :action => :index) %>
            </li>
          </ul>
          <div class="clear"></div>
        </div>
        <div class="sub_tab_container_content">
          <div id="tab-content" class="wizard" style="">
            <%= yield %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
</div>
<%= render :partial => 'navigation_elements/footer' %>
</body>
</html>
