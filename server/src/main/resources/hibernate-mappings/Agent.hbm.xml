<hibernate-mapping
  xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping https://hibernate.org/xsd/hibernate-mapping/hibernate-mapping-4.0.xsd"
  default-access="field" default-lazy="false">
    <class name="com.thoughtworks.go.config.Agent" table="agents" >
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="uuid"/>
        <property name="cookie"/>
        <property name="hostname"/>
        <property name="ipaddress"/>
        <property name="disabled"/>
        <property name="elasticAgentId"/>
        <property name="elasticPluginId"/>
        <property name="environments"/>
        <property name="resources"/>
        <property name="deleted"/>
    </class>
</hibernate-mapping>
