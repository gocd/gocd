define(['mithril'], function (m) {

  var AgentsTableHeader = {
    view: function (ctrl, args) {
      return (
        <thead style={args.style}>
        <tr>
          <td>
            <input type="checkbox"
                   checked={args.checkboxValue()}
                   onclick={m.withAttr('checked', args.onCheckboxClick)}
            />
          </td>
          <td>Agent Name</td>
          <td>Sandbox</td>
          <td>OS</td>
          <td>IP Address</td>
          <td>Status</td>
          <td>Free Space</td>
          <td>Resources</td>
          <td>Environments</td>
        </tr>
        </thead>
      );
    }
  };

  return AgentsTableHeader;
});
